<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroundlogo"
    tools:context=".Fragment.Completed_Order_Fragment">

    <!-- TODO: Update blank fragment layout -->

    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/rec_cancelled_order"/>

    <TextView
        android:layout_centerInParent="true"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txt_no_orders"
        android:text="Currently you have no orders"
        android:textSize="20sp"
        android:textColor="@color/text_color"
        android:fontFamily="@font/amiri_regular"
        android:layout_gravity="center"
        android:gravity="center"/>

</FrameLayout>